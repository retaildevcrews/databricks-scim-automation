{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "root": true,
    "rules": {
        "indent": "off",
        "max-len": "off",
        "no-underscore-dangle": "off",
        "no-unused-expressions": "off"
    }
}
